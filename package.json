{
	"name": "deku-tabs",
	"version": "4.3.0",
	"description": "Simple tab component for deku",
	"license": "MIT",
	"repository": "gillstrom/deku-tabs",
	"author": {
		"name": "Andreas GillstrÃ¶m",
		"email": "andreasgillstrom@gmail.com",
		"url": "github.com/gillstrom"
	},
	"engines": {
		"node": ">=0.10.0"
	},
	"scripts": {
		"build": "webpack --module-bind js=babel-loader example/index.js example/build.js",
		"prepublish": "babel index.js --out-file dist.js",
		"test": "xo && ava"
	},
	"main": "dist.js",
	"files": [
		"dist.js"
	],
	"keywords": [
		"component",
		"deku",
		"dekujs",
		"deku-component",
		"tab",
		"tabs"
	],
	"dependencies": {
		"deep-equal": "^1.0.1",
		"magic-virtual-element": "^1.0.6",
		"object-assign": "^4.1.0"
	},
	"devDependencies": {
		"assert-element": "^0.3.1",
		"ava": "*",
		"babel-cli": "^6.1.18",
		"babel-loader": "^7.1.2",
		"babel-preset-es2015": "^6.1.18",
		"babel-preset-react": "^6.1.18",
		"component-mock": "^0.1.0",
		"deku": "^1.0.0 ",
		"eslint-config-xo-react": "^0.13.0",
		"eslint-plugin-react": "^7.3.0",
		"webpack": "^3.5.5",
		"xo": "*"
	},
	"ava": {
		"babel": "inherit",
		"failWithoutAssertions": false,
		"require": [
			"babel-register"
		]
	},
	"babel": {
		"presets": [
			"es2015",
			"react"
		]
	},
	"xo": {
		"envs": [
			"browser",
			"node"
		],
		"extends": [
			"xo-react"
		],
		"ignores": [
			"dist.js",
			"example/build.js"
		],
		"rules": {
			"jsx-quotes": [
				2,
				"prefer-single"
			],
			"react/no-unknown-property": 0,
			"react/prop-types": 0,
			"quote-props": 0
		}
	}
}
